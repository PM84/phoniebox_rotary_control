#1.0.10
[Unit]
Description=Phoniebox Rotary Control Service
After=network.target iptables.service firewalld.service

[Service]
Type=simple
WorkingDirectory=<PATH>/scripts/
ExecStart=/usr/bin/python3 <PATH>/scripts/./phoniebox_rotary_control.py
Restart=always
RestartSec=3

[Install]
WantedBy=multi-user.target
